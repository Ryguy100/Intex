    <!-- SURVEY FORM -->

    <div class="sleek-header">
      <h1>Mental Health Survey</h1>
    </div>

    <div class="container-sm my-2">
      <form
        class="form-floating col-md-5 mx-auto my-5 needs-validation"
        id="surveyForm"
        method="post"
        action="/survey"
       novalidate>
        <!-- Question 1 -->

        <div class="form-floating form-group">
          <input
            type="number"
            class="form-control"
            id="age"
            name="age"
            min="0"
            max="130"
            placeholder="Age"
            required
          />
          <label for="age">Age</label>
          <div class="invalid-feedback">
            You must answer this question before submitting.
          </div>
        </div>

        <!-- Question 2 -->

        <div class="form-floating form-group">
          <select class="form-select" id="gender" name="gender" required>
            <option value="" disabled selected>Select an option</option>
            <option value="Male">Male</option>
            <option value="Female">Female</option>
            <option value="N-B">Non-Binary</option>
            <option value="Prefer not to say">Prefer Not to Say</option>
          </select>
          <label for="gender">Gender</label>
          <div class="invalid-feedback">
            You must answer this question before submitting.
          </div>
        </div>

        <!-- Question 3 -->

        <div class="form-group form-floating">
          <select
            class="form-select"
            id="relationshipStatus"
            name="relationshipStatus"
            required
          >
            <option value="" disabled selected>Select an option</option>
            <option value="single">Single</option>
            <option value="relationship">In a relationship</option>
            <option value="married">Married</option>
            <option value="divorced">Divorced</option>
          </select>
          <label for="relationshipStatus">Relationship Status</label>
          <div class="invalid-feedback">
            You must answer this question before submitting.
          </div>
        </div>

        <!-- Question 4 -->

        <div class="form-group form-floating">
          <select
            class="form-select"
            id="occupationStatus"
            name="occupationStatus"
            required
          >
            <option value="" disabled selected>Select an option</option>
            <option value="Student">Student</option>
            <option value="Salary_Worker">Salary Worker</option>
            <option value="Retired">Retired</option>
          </select>
          <label for="occupationStatus">Occupation Status</label>
          <div class="invalid-feedback">
            You must answer this question before submitting.
          </div>
        </div>

        <!-- Question 5 -->

        <div class="form-group">
          <label for="usesSocialMedia"
            >Do you use social media?</label
          >
          <div class="form-check">
            <input
              class="form-check-input"
              type="radio"
              name="usesSocialMedia"
              id="usesSocialMedia"
              value="1"
              required
            / >
            <label class="form-check-label" for="usesSocialMedia"
              >Yes</label
            >
          </div>
          <div class="form-check">
            <input
              class="form-check-input"
              type="radio"
              name="usesSocialMedia"
              id="usesSocialMedia"
              value="2"
            />
            <label class="form-check-label" for="usesSocialMedia"
              >No</label
            >
            <div class="invalid-feedback">
              You must answer this question before submitting.
            </div>
          </div>
        </div>

        <div class="form-group">
          <label
            >What social media platforms do you use? Select all that
            apply</label
          >
          <br />
          <input
            type="checkbox"
            id="instagram"
            name="socialMediaPlatforms"
            value="instagram"
            class="form-check-input checkbox-group"
          />
          <label for="instagram">Instagram</label><br />
          <input
            type="checkbox"
            id="facebook"
            name="socialMediaPlatforms"
            value="facebook"
            class="form-check-input checkbox-group"

          />
          <label for="facebook">Facebook</label><br />
          <input
            type="checkbox"
            id="youtube"
            name="socialMediaPlatforms"
            value="youtube"
            class="form-check-input checkbox-group"

          />
          <label for="youtube">Youtube</label><br />
          <input
            type="checkbox"
            id="discord"
            name="socialMediaPlatforms"
            value="discord"
            class="form-check-input checkbox-group"

          />
          <label for="discord">Discord</label><br />
          <input
            type="checkbox"
            id="twitter"
            name="socialMediaPlatforms"
            value="twitter"
            class="form-check-input checkbox-group"

          />
          <label for="twitter">Twitter</label><br />

          <input
            type="checkbox"
            id="pinterest"
            name="socialMediaPlatforms"
            value="pinterest"
            class="form-check-input checkbox-group"

          />
          <label for="pinterest">Pinterest</label><br />

          <input
            type="checkbox"
            id="reddit"
            name="socialMediaPlatforms"
            value="reddit"
            class="form-check-input checkbox-group"

          />
          <label for="reddit">Reddit</label>
          <br />
          <input
            type="checkbox"
            id="snapchat"
            name="socialMediaPlatforms"
            value="snapchat"
            class="form-check-input checkbox-group"

          />
          <label for="snapchat">Snapchat</label>
          <br />
          <input
            type="checkbox"
            id="snapchat"
            name="socialMediaPlatforms"
            value="None"
          class="form-check-input checkbox-group"

          />
          <label for="snapchat">None</label>
        <div class="invalid-feedback">
          You must answer this question before submitting.
        </div>
        </div>

        <!-- Question 8 -->

        <div class="form-group">
