              name="distractedBySocialMedia"
              id="distractedBySocialMedia"
              value="5"
            />
            <label class="form-check-label" for="distractedBySocialMedia"
              >5 - Always</label
            >
            <div class="invalid-feedback">
              You must answer this question before submitting.
            </div>
